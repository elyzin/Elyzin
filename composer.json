{
    "name": "effone/elyzin",
    "description": "PHP Discussion Forum",
    "type": "project",
    "require": {
        "phpmailer/phpmailer": "^6.0",
        "paragonie/constant_time_encoding": "^2.2",
        "izniburak/pdox": "^1",
        "ezimuel/php-secure-session": "^1.1",
        "philipp15b/php-i18n": "^4.0",
        "rakit/validation": "^1.2"
    },
    "config": {
        "vendor-dir": "vendor"
    },
    "require-dev": {
        "foxy/foxy": "^1.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "effone",
            "email": "me@eff.one"
        }
    ],
    "autoload": {
        "psr-4": {
            "Elyzin\\Core\\": "core",
            "Elyzin\\Core\\Traits\\": "core/traits",
            "Elyzin\\Model\\": "model",
            "Elyzin\\View\\": "view",
            "Elyzin\\Controller\\": "controller",
            "Elyzin\\Service\\": "service"
        },
        "classmap": [
            "core",
            "model",
            "controller",
            "service",
            "view"
        ]
    },
    "minimum-stability": "stable"
